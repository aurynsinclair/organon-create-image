{
  "name": "organon-create-image",
  "version": "0.1.0",
  "description": "MCP server for image generation using Gemini on Vertex AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
