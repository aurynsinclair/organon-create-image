    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/genai": "^1.43.0",
    "@modelcontextprotocol/sdk": "^1.27.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.8.0",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "license": "MIT"
}
